{
  "name": "vortex",
  "version": "0.0.1",
  "description": "VortΞx is an Ethereum Dapp redux store that handles transactions, smart contracts, events ...",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "jest": {
    "globalSetup": "./setup/setup.js",
    "globalTeardown": "./setup/teardown.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "npm": "^5.8.0",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "ganache-core": "^2.1.0",
    "jest": "^22.4.3",
    "linted": "0.0.2",
    "truffle": "^4.1.7",
    "ts-jest": "^22.4.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1"
  },
  "scripts": {
    "build": "tslint --project . && tsc",
    "test": "tslint --project . && jest",
    "doc": "rm -rf ./doc && tslint --project . && tsc && typedoc --out doc --mode file ./_sources/sources/ --readme ./README.md --name \"VortΞx\" --theme minimal --hideGenerator --target ES6"
  },
  "repository": {
    "type": "git",
    "url": "https//gitlab.com/mortimr/vortex"
  },
  "keywords": [
    "ethereum",
    "redux",
    "smart",
    "contract",
    "transaction"
  ],
  "author": "Iulian Rotaru",
  "license": "ISC",
  "bugs": {
    "url": "https//gitlab.com/mortimr/vortex/issues"
  },
  "homepage": "https//gitlab.com/mortimr/vortex#README"
}
